    <div class="col-lg-12 p-r-0 p-l-0">
        <div class="card alert card-section-payment">
            <div class="card-header">
                <h4>Payment Summary</h4>
            </div>
            <div id="invoice-bot">
                <div id="invoice-table">
                    <div id="divInvoice">

                    </div>
                </div>
                <!--End Table-->
                <button type="button" class="btn btn-primary m-l-5 m-t-15">Print Receipt</button>
                <button type="button" class="btn btn-primary m-l-5 m-t-15">Print Folio</button>
            </div>
        </div>
    </div>
    <script type="text/html" id="invoiceTemplate">
        <table class="table table-responsive">
            <tbody>
                <tr class="tabletitle">
                    <td class="table-item">
                        <b>Item Description</b>
                    </td>
                    <td class="subtotal">
                        <b>Sub-total</b>
                    </td>
                </tr>
    {{each Tax}}
                <tr class="service">
                    <td class="tableitem">${$value.TaxName}</td>
                    <td class="tableitem">
                        {{if $value.IsEnabled }}
                        $<input id="taxVal" name="${$value.TaxName}" onkeyup="OnTaxValChange(event, this)" value="${$value.Value}" />
                        {{else}}
                        $<input id="taxVal" disabled="disabled" name="${$value.TaxName}" onkeyup="OnTaxValChange(event, this)" value="${$value.Value}" />
                        {{/if}}
                        
                    </td>
                </tr>
    {{/each}}
        <tr class="service">
            <td class="tableitem">
                <span class="color-danger">Discount</span>
            </td>
            <td class="tableitem">
                $<input id="discount" onkeyup="OnTaxValChange(event, this)" class="color-danger" value="" />
            </td>
        </tr>
        <tr class="tabletitle">
            <td class="Rate">
                <b>Total</b>
            </td>
            <td id="total" class="payment">
                <b class=""></b>
            </td>
        </tr>
    </tbody>
</table>
</script>

